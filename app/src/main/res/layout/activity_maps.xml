<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <fragment
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MapsActivity"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btnPeriode"
        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        app:cornerRadius="28dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.317"
        app:icon="@drawable/calendar"
        app:iconTintMode="multiply"
        app:iconTint="@null"
        app:iconPadding="0dp"
        android:insetBottom="0dp"
        android:insetRight="0dp"
        android:insetTop="0dp"
        android:insetLeft="0dp"
        app:iconSize="48dp"
        android:padding="0dp"
        tools:ignore="InvalidId,MissingConstraints"
        app:iconGravity="textStart"
        />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btnCoordonnesGps"
        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="140dp"
        android:text="Gps"
        app:iconSize="48dp"
        app:iconGravity="textStart"
        app:cornerRadius="28dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:iconTintMode="multiply"
        app:icon="@drawable/localiser"
        app:iconTint="@null"
        app:iconPadding="0dp"
        android:insetBottom="0dp"
        android:insetRight="0dp"
        android:insetTop="0dp"
        android:insetLeft="0dp"

        android:padding="0dp"
        tools:ignore="InvalidId,MissingConstraints" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btnAddConsomation"
        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="36dp"
        android:text="Ajouts consomation"
        app:iconSize="48dp"
        app:iconGravity="textStart"
        app:iconTintMode="multiply"
        app:icon="@drawable/elec"
        app:iconTint="@null"
        app:iconPadding="0dp"
        android:insetBottom="0dp"
        android:insetRight="0dp"
        android:insetTop="0dp"
        android:insetLeft="0dp"

        android:padding="0dp"
        app:cornerRadius="28dp"
        app:layout_constraintBottom_toTopOf="@+id/btnMonthly"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="InvalidId,MissingConstraints" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btnMonthly"
        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        app:cornerRadius="28dp"
        app:iconGravity="textStart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.629"
        app:icon="@drawable/calendar_date"
        app:iconSize="48dp"
        app:iconPadding="0dp"
        android:insetBottom="0dp"
        android:insetRight="0dp"
        android:insetTop="0dp"
        android:insetLeft="0dp"

        android:padding="0dp"
        app:iconTint="@color/white"

        tools:ignore="InvalidId,MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>